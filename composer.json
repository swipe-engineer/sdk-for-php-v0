{
    "name": "swipe-engineer/sdk-for-php-v0",
    "description": "SWIPE SDK for PHP - Use Swipe Web Services in your PHP project",
    "keywords": [
        "swipe",
        "swipego",
        "sdk"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Swipe Web Services",
            "homepage": "https://swipego.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Api\\Client\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php-http/client-common": "^2.5",
        "php-http/discovery": "^1.14",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.0",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.0",
        "laminas/laminas-diactoros": "^2.11",
        "php-http/curl-client": "^2.2"
    }
}
